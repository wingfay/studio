        <th>
            @Html.DisplayNameFor(model => model.PersonNum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Keywrod)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
